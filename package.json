{
  "name": "nine-editor",
  "version": "0.1.0",
  "description": "nine editor base on quill",
  "private": false,
  "bugs": {
    "url": "https://github.com/NineSwordsMonster/nine-editor/issues"
  },
  "homepage": "https://github.com/NineSwordsMonster/nine-editor#readme",
  "main": "dist/nine-editor.js",
  "unpkg": "dist/nine-editor.js",
  "jsnext:main": "dist/nine-editor.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "nine-editor",
    "vue quill",
    "vue text editor",
    "vue rich text editor",
    "vue editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/NineSwordsMonster/nine-editor.git"
  },
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target wc --name nine-editor --dest dist src/index.js",
    "lint": "vue-cli-service lint",
    "publish": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "lodash.merge": "^4.6.1",
    "quill": "^1.3.6",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
